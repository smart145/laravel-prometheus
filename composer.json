{
    "name": "smart145/laravel-prometheus",
    "description": "Prometheus metrics for Laravel applications",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/smart145/laravel-prometheus",
    "support": {
        "issues": "https://github.com/smart145/laravel-prometheus/issues",
        "source": "https://github.com/smart145/laravel-prometheus"
    },
    "keywords": [
        "laravel",
        "prometheus",
        "metrics",
        "monitoring"
    ],
    "require": {
        "php": "^8.2",
        "promphp/prometheus_client_php": "^2.6",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Smart145\\Prometheus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smart145\\Prometheus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smart145\\Prometheus\\PrometheusServiceProvider"
            ],
            "aliases": {
                "Prometheus": "Smart145\\Prometheus\\Facades\\Prometheus"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

